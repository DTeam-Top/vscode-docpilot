{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "types": ["node", "@playwright/test"],
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true
  },
  "include": [
    "src/test/e2e/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}